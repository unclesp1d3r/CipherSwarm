<h2>Users</h2>
<table class="table table-striped table-bordered table-sm">
  <thead>
  <tr>
    <th>id</th>
    <th>User Name</th>
    <th>Email</th>
    <th>Role</th>
  </tr>
  </thead>
  <tbody class="table-group-divider">
  <% @users.each do |u| %>
    <tr>
      <td><%= u.id %></td>
      <td><%= u.name %></td>
      <td><%= u.email %></td>
      <td><%= u.role %></td>
    </tr>
  <% end %>
  </tbody>
</table>
