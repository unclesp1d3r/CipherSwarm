<h1><%= title @mask_list.name %></h1>
<%= show_for @mask_list do |s| %>
  <%= s.attribute :name %>
  <%= s.attribute :description %>
  <%= s.attribute :line_count %>
  <%= s.attribute :sensitive %>
  <%= s.association :projects %>
<% end %>
<div class="btn-group">
  <%= link_to "Edit", edit_mask_list_path(@mask_list), class: "btn btn-warning" %>
  <%= link_to "Back", mask_lists_path, class: "btn btn-outline-secondary" %>
</div>
