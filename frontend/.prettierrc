{
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "quoteProps": "as-needed",
    "printWidth": 100,
    "bracketSameLine": true,
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
